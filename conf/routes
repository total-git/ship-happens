# Routes
# Defines all routes from address to application part
# Higher priority routes come first

GET     /                   controllers.Application.index

GET     /player/:id         controllers.Player.index(id: Int)

GET     /action/move/:id    controllers.Player.move(id: Int)

GET     /api/getOwn/:id     controllers.Api.getOwn(id: Int)
#POST    /api/move/:id       controllers.Api.move(id: Int)

GET     /assets/*file       controllers.Assets.at(path="/public", file)